{
  "name": "@xg4/node-aes",
  "version": "0.0.4",
  "description": "crypto AES",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/types/index.d.ts",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "jest",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "release": "npm run build && git commit -am \"v\"$npm_package_version && git tag \"v\"$npm_package_version && git push && git push --tags && npm publish --access"
  },
  "keywords": [
    "crypto",
    "AES"
  ],
  "author": "xg4 <xingor4@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.5",
    "jest": "^24.1.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "typescript": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xg4/node-aes.git"
  },
  "bugs": {
    "url": "https://github.com/xg4/node-aes/issues"
  },
  "homepage": "https://github.com/xg4/node-aes#readme"
}
